{
  "name": "rest-api-node",
  "version": "0.0.1",
  "description": "REST API with node.js",
  "main": "app.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel --out-dir ./build . --source-maps --copy-files --ignore 'node_modules/**/*.js'",
    "build": "npm run clean && npm run build-server",
    "start": "node ./build/app.js -i ${NODE_PROCESSES}",
    "dev": "NODE_ENV=development nodemon --exec babel-node app.js",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LOXMAN/rest-api-node-js.git"
  },
  "author": "tayakorn_s",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LOXMAN/rest-api-node-js/issues"
  },
  "homepage": "https://github.com/LOXMAN/rest-api-node-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "express": "^4.18.2"
  }
}
